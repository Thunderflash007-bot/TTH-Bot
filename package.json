{
  "name": "tth-bot",
  "version": "1.0.0",
  "description": "Multifunktionaler Discord-Management-Bot mit Web-Dashboard",
  "main": "bot/index.js",
  "scripts": {
    "start": "node bot/index.js",
    "interface": "node interface/server.js",
    "dev": "nodemon bot/index.js",
    "dev:interface": "nodemon interface/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:interface\"",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status"
  },
  "keywords": [
    "discord",
    "bot",
    "management",
    "dashboard"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
